{
  "pangaea:condition": {
    "mods": [ "catlib", "quark" ],
    "registry": {
      "pangaea:injector": [ "pangaea:biome/forest_like" ]
    },
    "filter": {
      "worldgen/density_function": [ "@pangaea" ]
    }
  },
  "biomes": [
    "#is_forest"
  ],
  "step": "vegetal_decoration",
  "inject": [
    {
      "type": "pangaea:giant_sphere",
      "placer": {
        "target": "#overworld_carver_replaceables",
        "place": [
          { "place": "green_stained_glass", "chance": 0.01 },
          { "place": "light_gray_stained_glass" }
        ]
      },
      "chunk_filter": 0.1,
      "step": "raw_generation"
    },
    {
      "type": "pangaea:test",
      "column": [ 0, 100 ],
      "placer": {
        "target": [ "air", "water" ],
        "place": [
          { "column": [ 0, 20 ], "place": "glowstone" },
          { "column": [ 20, 40 ], "place": "sea_lantern" },
          { "column": [ 40, 60 ], "place": "glowstone" },
          { "column": { "surface": 1 }, "place": "redstone_block" },
          { "column": [ 60, 80 ], "place": "sea_lantern" },
          { "column": [ 80, 100 ], "place": "diamond_block", "chance": 0.5 },
          { "column": [ 80, 100 ], "place": "gold_block" }
        ]
      }
    },
    {
     "type": "ore",
     "discard_chance_on_air_exposure": 0,
     "size": 20,
     "targets": [
       {
         "state": "diamond_ore",
         "target": [ "grass", "dirt" ]
       }
     ],
      "placement": [
        {
          "count": 20,
          "height": [ 50, 100 ]
        }
      ]
    }
  ]
}